  "entryPoints": ["src/index.ts"],
  "sort": ["source-order"],
  "categoryOrder": ["Number", "Currency", "Date & Time", "*"],
  "plugin": ["typedoc-plugin-markdown", "typedoc-github-wiki-theme"],
  "readme": "none",
  "enumMembersFormat": "table",
